    <div class="modal fade" id="addLabQualityControlModel" tabindex="-1" role="dialog" aria-labelledby="addLabQualityControlModalLabel" aria-hidden="true" data-backdrop="static">
        <div class="modal-dialog custom_modal_dialog">
            <div class="modal-content">
                <form id="add_lqc_form" class="form-horizontal" method="post" enctype="multipart/form-data" action="/Admin/Home/ProductsManage/AddLaboratoryQuailtyControl">
